        <rdfs:range rdf:resource="&rdfs;Literal"/>
        <rdfs:domain rdf:resource="&foaf;Agent"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ontology/myspace#friend -->

    <owl:ObjectProperty rdf:about="&myspace;friend">
