<?xml version="1.0"?>


<!DOCTYPE rdf:RDF [
    <!ENTITY foaf "http://xmlns.com/foaf/0.1/" >
    <!ENTITY mo "http://purl.org/ontology/mo/" >
    <!ENTITY owl "http://www.w3.org/2002/07/owl#" >
    <!ENTITY owl11 "http://www.w3.org/2006/12/owl11#" >
    <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#" >
    <!ENTITY owl11xml "http://www.w3.org/2006/12/owl11-xml#" >
    <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#" >
    <!ENTITY wgs84_pos "http://www.w3.org/2003/01/geo/wgs84_pos#" >
    <!ENTITY myspace "http://grasstunes.net/ontology/myspace.owl#" >
    <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#" >
]>


<rdf:RDF xmlns="http://grasstunes.net/ontology/myspace.owl#"
     xml:base="http://grasstunes.net/ontology/myspace.owl"
     xmlns:owl11="http://www.w3.org/2006/12/owl11#"
     xmlns:owl11xml="http://www.w3.org/2006/12/owl11-xml#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:wgs84_pos="http://www.w3.org/2003/01/geo/wgs84_pos#"
     xmlns:mo="http://purl.org/ontology/mo/"
     xmlns:myspace="http://grasstunes.net/ontology/myspace.owl#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:foaf="http://xmlns.com/foaf/0.1/"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:owl="http://www.w3.org/2002/07/owl#">
    <owl:Ontology rdf:about="">
        <owl:imports rdf:resource="http://www.geonames.org/ontology"/>
        <rdfs:comment
            >An ontology for describing MySpace stuff, especially artists</rdfs:comment>
        <owl:imports rdf:resource="http://www.w3.org/2003/01/geo/wgs84_pos#"/>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://grasstunes.net/ontology/myspace.owl#age -->

    <owl:ObjectProperty rdf:about="#age">
        <rdfs:range rdf:resource="&rdfs;Literal"/>
        <rdfs:domain rdf:resource="&foaf;Agent"/>
        <rdfs:comment
            >MySpace age relationship - an age of a MySpace agent in years</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://grasstunes.net/ontology/myspace.owl#friend -->

    <owl:ObjectProperty rdf:about="#friend">
        <rdf:type rdf:resource="&owl;SymmetricProperty"/>
        <rdfs:subPropertyOf rdf:resource="&foaf;knows"/>
        <rdfs:range rdf:resource="&foaf;Person"/>
        <rdfs:comment
            >MySpace friend relationship - undirected</rdfs:comment>
        <rdfs:domain rdf:resource="&foaf;Person"/>
    </owl:ObjectProperty>
    


    <!-- http://grasstunes.net/ontology/myspace.owl#genreTag -->

    <owl:ObjectProperty rdf:about="#genreTag">
        <rdf:type rdf:resource="&owl11;AntisymmetricProperty"/>
        <rdfs:comment
            >for the genres specified by artists on MySpace</rdfs:comment>
        <rdfs:range rdf:resource="&mo;Genre"/>
        <rdfs:domain rdf:resource="#MusicArtist"/>
    </owl:ObjectProperty>
    


    <!-- http://grasstunes.net/ontology/myspace.owl#topFriend -->

    <owl:ObjectProperty rdf:about="#topFriend">
        <rdf:type rdf:resource="&owl11;AntisymmetricProperty"/>
        <rdfs:comment
            >MySpace top friend relationship - a directed relationship</rdfs:comment>
        <rdfs:domain rdf:resource="#MusicArtist"/>
        <rdfs:subPropertyOf rdf:resource="&foaf;knows"/>
        <rdfs:domain rdf:resource="&foaf;Agent"/>
        <rdfs:range rdf:resource="&foaf;Agent"/>
        <rdfs:range rdf:resource="#MusicArtist"/>
    </owl:ObjectProperty>
    


    <!-- http://xmlns.com/foaf/0.1/knows -->

    <owl:ObjectProperty rdf:about="&foaf;knows"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://grasstunes.net/ontology/myspace.owl#bandMembers -->

    <owl:DatatypeProperty rdf:about="#bandMembers">
        <rdfs:range rdf:resource="&xsd;string"/>
        <rdfs:domain rdf:resource="#MusicArtist"/>
    </owl:DatatypeProperty>
    


    <!-- http://grasstunes.net/ontology/myspace.owl#cachedMP3 -->

    <owl:DatatypeProperty rdf:about="#cachedMP3">
        <rdfs:domain rdf:resource="&mo;AudioFile"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


    <!-- http://grasstunes.net/ontology/myspace.owl#influences -->

    <owl:DatatypeProperty rdf:about="#influences">
        <rdfs:domain rdf:resource="#MusicArtist"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


    <!-- http://grasstunes.net/ontology/myspace.owl#plays -->

    <owl:DatatypeProperty rdf:about="#plays">
        <rdfs:domain rdf:resource="&mo;AudioFile"/>
        <rdfs:comment
            >number of plays for a track</rdfs:comment>
        <rdfs:range rdf:resource="&xsd;int"/>
    </owl:DatatypeProperty>
    


    <!-- http://grasstunes.net/ontology/myspace.owl#profileViews -->

    <owl:DatatypeProperty rdf:about="#profileViews">
        <rdfs:domain rdf:resource="#Page"/>
        <rdfs:range rdf:resource="&xsd;int"/>
    </owl:DatatypeProperty>
    


    <!-- http://grasstunes.net/ontology/myspace.owl#recordLabel -->

    <owl:DatatypeProperty rdf:about="#recordLabel">
        <rdfs:domain rdf:resource="#MusicArtist"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


    <!-- http://grasstunes.net/ontology/myspace.owl#recordLabelType -->

    <owl:DatatypeProperty rdf:about="#recordLabelType">
        <rdfs:domain rdf:resource="#MusicArtist"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


    <!-- http://grasstunes.net/ontology/myspace.owl#soundsLike -->

    <owl:DatatypeProperty rdf:about="#soundsLike">
        <rdfs:range rdf:resource="&xsd;string"/>
        <rdfs:domain rdf:resource="#MusicArtist"/>
    </owl:DatatypeProperty>
    


    <!-- http://grasstunes.net/ontology/myspace.owl#totalPlays -->

    <owl:DatatypeProperty rdf:about="#totalPlays">
        <rdfs:range rdf:resource="&xsd;integer"/>
        <rdfs:range rdf:resource="&xsd;int"/>
        <rdfs:domain rdf:resource="#MusicArtist"/>
    </owl:DatatypeProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://grasstunes.net/ontology/myspace.owl#Agent -->

    <owl:Class rdf:about="#Agent">
        <rdfs:subClassOf rdf:resource="&foaf;Agent"/>
        <rdfs:comment>MySpace agent</rdfs:comment>
    </owl:Class>
    


    <!-- http://grasstunes.net/ontology/myspace.owl#AudioFile -->

    <owl:Class rdf:about="#AudioFile">
        <rdfs:subClassOf rdf:resource="&mo;AudioFile"/>
        <rdfs:comment
            >An audio file on a MySpace artist page</rdfs:comment>
    </owl:Class>
    


    <!-- http://grasstunes.net/ontology/myspace.owl#Country -->

    <owl:Class rdf:about="#Country">
        <rdfs:subClassOf rdf:resource="&wgs84_pos;SpatialThing"/>
    </owl:Class>
    


    <!-- http://grasstunes.net/ontology/myspace.owl#Genre -->

    <owl:Class rdf:about="#Genre">
        <rdfs:subClassOf rdf:resource="&mo;Genre"/>
        <rdfs:comment
            >MySpace genre from the list of 119</rdfs:comment>
    </owl:Class>
    


    <!-- http://grasstunes.net/ontology/myspace.owl#MusicArtist -->

    <owl:Class rdf:about="#MusicArtist">
        <rdfs:subClassOf rdf:resource="&mo;MusicArtist"/>
        <rdfs:subClassOf rdf:resource="&foaf;Person"/>
        <rdfs:comment
            >MySpace Artist page</rdfs:comment>
    </owl:Class>
    


    <!-- http://grasstunes.net/ontology/myspace.owl#Page -->

    <owl:Class rdf:about="#Page">
        <rdfs:subClassOf rdf:resource="&foaf;Agent"/>
        <rdfs:subClassOf rdf:resource="&foaf;Document"/>
        <rdfs:comment>MySpace page</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/ontology/mo/AudioFile -->

    <owl:Class rdf:about="&mo;AudioFile"/>
    


    <!-- http://purl.org/ontology/mo/Genre -->

    <owl:Class rdf:about="&mo;Genre"/>
    


    <!-- http://purl.org/ontology/mo/MusicArtist -->

    <owl:Class rdf:about="&mo;MusicArtist"/>
    


    <!-- http://www.w3.org/2000/01/rdf-schema#Literal -->

    <owl:Class rdf:about="&rdfs;Literal"/>
    


    <!-- http://www.w3.org/2003/01/geo/wgs84_pos#SpatialThing -->

    <owl:Class rdf:about="&wgs84_pos;SpatialThing"/>
    


    <!-- http://xmlns.com/foaf/0.1/Agent -->

    <owl:Class rdf:about="&foaf;Agent"/>
    


    <!-- http://xmlns.com/foaf/0.1/Document -->

    <owl:Class rdf:about="&foaf;Document"/>
    


    <!-- http://xmlns.com/foaf/0.1/Person -->

    <owl:Class rdf:about="&foaf;Person">
        <rdfs:subClassOf rdf:resource="&foaf;Agent"/>
    </owl:Class>
</rdf:RDF>
