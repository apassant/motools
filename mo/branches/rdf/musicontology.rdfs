        <owl:versionInfo>Revision: 1.2</owl:versionInfo>
        <foaf:maker rdf:resource="http://moustaki.org/foaf.rdf#moustaki"/>
        <foaf:maker rdf:resource="http://www.talkdigger.com/foaf/fgiasson"/>
    </owl:Ontology>

