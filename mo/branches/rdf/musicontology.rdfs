                <rdf:rest rdf:parseType="Resource">
                    <rdf:first rdf:resource="http://purl.org/ontology/mo/Score"/>
                    <rdf:rest rdf:parseType="Resource">
                        <rdf:first rdf:resource="http://purl.org/ontology/mo/Libretto"/>
                        <rdf:rest rdf:parseType="Resource">
                            <rdf:first rdf:resource="http://purl.org/ontology/mo/Lyrics"/>
                            <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                        </rdf:rest>
                    </rdf:rest>
                </rdf:rest>
            </owl:unionOf>
        </domain>
        <isDefinedBy rdf:resource="http://purl.org/ontology/mo/"/>
        <label>published as</label>
        <range rdf:parseType="Resource">
            <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
            <owl:unionOf rdf:parseType="Resource">
                <rdf:first rdf:resource="http://purl.org/ontology/mo/Record"/>
                <rdf:rest rdf:parseType="Resource">
                    <rdf:first rdf:resource="http://purl.org/ontology/mo/PublishedScore"/>
                    <rdf:rest rdf:parseType="Resource">
                        <rdf:first rdf:resource="http://purl.org/ontology/mo/PublishedLibretto"/>
                        <rdf:rest rdf:parseType="Resource">
                            <rdf:first rdf:resource="http://purl.org/ontology/mo/PublishedLyrics"/>
