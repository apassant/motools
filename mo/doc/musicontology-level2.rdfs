    mo:level="2"
    rdfs:label="produced signal group"
    ns1:term_status="testing">
  <rdfs:comment>Associates a recording session with a group of master signals produced by it.</rdfs:comment>
  <rdfs:domain rdf:resource="&mo;RecordingSession"/>
  <rdfs:isDefinedBy rdf:resource="&mo;"/>
