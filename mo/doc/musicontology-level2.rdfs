  <rdfs:range rdf:resource="&mo;AnalogSignal"/>
  <rdfs:subPropertyOf rdf:resource="&mo;derived_from"/>
  <owl:inverseOf rdf:resource="&mo;sampled_version"/>
</owl:FunctionalProperty>

