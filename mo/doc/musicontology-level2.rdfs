  <rdfs:subPropertyOf rdf:resource="http://purl.org/vocab/bio/0.1/event"/>
  <rdfs:domain rdf:resource="http://xmlns.com/foaf/0.1/Agent"/>
  <rdfs:isDefinedBy rdf:resource="&mo;"/>
  <rdfs:range rdf:resource="&mo;Performance"/>
  <owl:inverseOf rdf:resource="&mo;performer"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="&mo;performed_in"
    mo:level="2"
    rdfs:label="performed in"
    ns1:term_status="unstable">
  <rdfs:comment>        Associates a Musical Work or an Score to Performances in which they were
        a factor. For example, I might use this property in order to 
        associate the Magic Flute to a particular performance at the Opera
        Bastille last year.</rdfs:comment>
  <rdfs:domain>
    <owl:Class>
      <owl:unionOf>
        <rdf:Description>
          <rdf:first rdf:resource="&mo;Score"/>
          <rdf:rest>
            <rdf:Description>
              <rdf:first rdf:resource="&mo;MusicalWork"/>
              <rdf:rest rdf:resource="&rdf;nil"/>
            </rdf:Description>
          </rdf:rest>
        </rdf:Description>
      </owl:unionOf>
    </owl:Class>
  </rdfs:domain>
  <rdfs:isDefinedBy rdf:resource="&mo;"/>
  <rdfs:range rdf:resource="&mo;Performance"/>
  <rdfs:subPropertyOf rdf:resource="http://purl.org/NET/c4dm/event.owl#isFactorOf"/>
  <owl:inverseOf rdf:resource="&mo;performance_of"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="&mo;performer"
    mo:level="2"
