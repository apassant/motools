  <rdfs:domain rdf:resource="&mo;Performance"/>
  <rdfs:isDefinedBy rdf:resource="&mo;"/>
  <rdfs:range rdf:resource="http://xmlns.com/foaf/0.1/Agent"/>
  <rdfs:subPropertyOf rdf:resource="&mo;performer"/>
</owl:ObjectProperty>

<owl:DatatypeProperty rdf:about="&mo;tempo"
    mo:level="2"
    rdfs:label="tempo"
    ns1:term_status="stable">
  <rdfs:comment>        Rate of speed or pace of music. Tempo markings are traditionally given in Italian; 
        common markings include: grave (solemn; very, very slow); largo (broad; very slow); 
        adagio (quite slow); andante (a walking pace); moderato (moderate); allegro (fast; cheerful); 
        vivace (lively); presto (very fast); accelerando (getting faster); ritardando (getting slower); 
        and a tempo (in time; returning to the original pace).
    </rdfs:comment>
  <rdfs:domain>
    <owl:Class>
      <owl:unionOf>
        <rdf:Description>
          <rdf:first rdf:resource="&mo;MusicalWork"/>
          <rdf:rest>
            <rdf:Description>
              <rdf:first rdf:resource="&mo;Performance"/>
              <rdf:rest>
                <rdf:Description>
                  <rdf:first rdf:resource="&mo;Signal"/>
                  <rdf:rest rdf:resource="&rdf;nil"/>
                </rdf:Description>
              </rdf:rest>
            </rdf:Description>
          </rdf:rest>
        </rdf:Description>
      </owl:unionOf>
    </owl:Class>
  </rdfs:domain>
  <rdfs:isDefinedBy rdf:resource="&mo;"/>
  <rdfs:range rdf:resource="&xsd;string"/>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="&mo;text"
    mo:level="2"
    rdfs:comment="Associates lyrics with their text."
    rdfs:label="text"
    ns1:term_status="testing">
  <rdfs:domain rdf:resource="&mo;Lyrics"/>
  <rdfs:isDefinedBy rdf:resource="&mo;"/>
</owl:DatatypeProperty>

