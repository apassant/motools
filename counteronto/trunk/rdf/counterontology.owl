<?xml version="1.0" encoding="utf-8"?>
<rdf:RDF
   xmlns:co="http://purl.org/ontology/co/"
   xmlns:dc="http://purl.org/dc/elements/1.1/"
   xmlns:dcterms="http://purl.org/dc/terms/"
   xmlns:event="http://purl.org/NET/c4dm/event.owl#"
   xmlns:foaf="http://xmlns.com/foaf/0.1/"
   xmlns:owl="http://www.w3.org/2002/07/owl#"
   xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
   xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
   xmlns:vs="http://www.w3.org/2003/06/sw-vocab-status/ns#"
   xmlns:xsd="http://www.w3.org/2001/XMLSchema#">
  <owl:Ontology rdf:about="http://purl.org/ontology/co/">
    <dc:date rdf:datatype="http://www.w3.org/2001/XMLSchema#string">$Date: 2010/06/24 17:48:19 $</dc:date>
    <dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The Counter Ontology specification provides basic concepts and properties 
for describing a general counter concept and some important sub counters, e.g. an event counter, on/ for 
the Semantic Web. This document contains a RDF description of the Counter Ontology.</dc:description>
    <dc:title>The Counter Ontology</dc:title>
    <owl:imports rdf:resource="http://purl.org/NET/c4dm/event.owl"/>
    <owl:imports rdf:resource="http://purl.org/dc/elements/1.1/"/>
    <owl:imports rdf:resource="http://purl.org/dc/terms/"/>
    <owl:imports rdf:resource="http://xmlns.com/foaf/0.1/"/>
    <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Revision: 0.2</owl:versionInfo>
    <foaf:maker rdf:resource="http://foaf.me/intrz#me"/>
    <foaf:maker rdf:resource="http://foaf.me/zazi#me"/>
    <foaf:maker rdf:resource="http://moustaki.org/foaf.rdf#moustaki"/>
  </owl:Ontology>
  <owl:Class rdf:about="http://purl.org/ontology/co/Counter">
    <rdfs:comment>Counter of a given object in a given media</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ontology/co/"/>
    <rdfs:label>Counter</rdfs:label>
    <vs:term_status rdf:datatype="http://www.w3.org/2001/XMLSchema#string">stable</vs:term_status>
  </owl:Class>
  <rdf:Description rdf:about="http://purl.org/ontology/co/ScrobbleEvent">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">An event corresponding to a scrobbling (counting an activity, which includes at least a specific object and 
a specific agent), e.g. a track being played by someone, a video watched by someone, a book read by someone, ...</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ontology/co/"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Scrobble Event</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://purl.org/NET/c4dm/event.owl#Event"/>
    <vs:term_status rdf:datatype="http://www.w3.org/2001/XMLSchema#string">unstable</vs:term_status>
  </rdf:Description>
  <owl:DatatypeProperty rdf:about="http://purl.org/ontology/co/count">
    <rdfs:comment>Links a counter resource to the actual count</rdfs:comment>
    <rdfs:domain rdf:resource="http://purl.org/ontology/co/Counter"/>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ontology/co/"/>
    <rdfs:label>count</rdfs:label>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
    <vs:term_status rdf:datatype="http://www.w3.org/2001/XMLSchema#string">stable</vs:term_status>
  </owl:DatatypeProperty>
  <owl:ObjectProperty rdf:about="http://purl.org/ontology/co/counter">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Links an object to a counter resource. Please feel free to create further sub properties with more restricted domains.</rdfs:comment>
    <rdfs:domain rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ontology/co/"/>
    <rdfs:label>counter</rdfs:label>
    <rdfs:range rdf:resource="http://purl.org/ontology/co/Counter"/>
    <owl:InverseOf>
      <owl:ObjectProperty>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">is counter of</rdfs:label>
      </owl:ObjectProperty>
    </owl:InverseOf>
    <vs:term_status rdf:datatype="http://www.w3.org/2001/XMLSchema#string">stable</vs:term_status>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://purl.org/ontology/co/event_counter">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Links events to a counter resource, where they where counted in.</rdfs:comment>
    <rdfs:domain rdf:resource="http://purl.org/NET/c4dm/event.owl#Event"/>
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ontology/co/"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">event counter</rdfs:label>
    <rdfs:subPropertyOf rdf:resource="http://purl.org/ontology/co/counter"/>
    <owl:InverseOf>
      <owl:ObjectProperty>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">is event counter of</rdfs:label>
      </owl:ObjectProperty>
    </owl:InverseOf>
    <vs:term_status rdf:datatype="http://www.w3.org/2001/XMLSchema#string">unstable</vs:term_status>
  </owl:ObjectProperty>
</rdf:RDF>
